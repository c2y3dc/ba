		script.
			var w = "864";
			var h = "486";
			//- $(window).on('resize',function(){location.reload();});
			var width = $(window).width();
			if (width < 975) {
				if(width > 768){
					w = "568";
					h = "320";
				}else if(width > 400){
					w = "550";
					h = "310";
				}else if (width > 350){
					w = "300";
					h = "169";
				}else{
					w = "213";
					h = "120";
				}
			}
			var tag = document.createElement('script');
			tag.src = "https://www.youtube.com/iframe_api";
			var firstScriptTag = document.getElementsByTagName('script')[0];
			firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

			var vids = {
				0 : "L8osE9bBeKg",
				1 : "zj2EoTkpz1k",
				2 : "iS_eQe7AfAY",
				3 : "uRA5gLfwjMo",
				4 : "7-WEzMqGjTo",
				5 : "0aOX_W8zZMA"
			}
			var players = [];

			function onYouTubeIframeAPIReady(){
				for(var vid in vids){
					var player = new YT.Player('player' + vid, {
						width: w,
						height: h,
						videoId: vids[vid],
						playerVars: {rel: 0},
						enablejsapi: 1
					});
					players.push(player);
				}
			}
			function stopVideo(i){
				players[i].stopVideo();
			}
			$("#myModal0").on('shown.bs.modal', function() {
				if (typeof players[0].playVideo == 'function') {
					players[0].playVideo();
				} else {
				var fn = function() {
					players[0].playVideo();
				};
					setTimeout(fn, 200);
				}
			});

			$("#myModal0").on('hidden.bs.modal', function(e) {
				players[0].stopVideo(0);
