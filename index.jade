		script.
			window.sr = new scrollReveal();
			function toggle(id){
				$("#" + id).toggle();
			}
			function togglevis(target){
				var ele = document.getElementById(target);
				if(ele.style.display == "block"){
					ele.style.display = "none";
				}else{ 
					ele.style.display = "block";
				}				
